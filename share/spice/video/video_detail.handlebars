{{! Iframe}}<iframe id="embed" src="{{embedURL provider id}}" frameborder="0" allowfullscreen></iframe>{{! Title}}<div>  <a href='{{playURL provider id}}' title="{{title}}" id="video-title">{{title}}</a></div>{{! Username}}<div class="description">  from <a href='{{userURL provider uploader}}'>{{display_name}}</a></div>{{! Statistics}}{{#checkStatistics statistics.viewCount}}  <div class="description">{{formatViews viewCount}} views</div>{{/checkStatistics}}{{! Only show this part if the video is in the category "Music"}}{{#checkMusic category title}}  <div class="links">    <a href='/?q=!spotify {{title}}' class='app_platform'>    <img src="https://icons.duckduckgo.com/i/www.spotify.com.ico" class="favicon">    Spotify  </a>    <a href='/?q=!a {{title}}' class='app_platform'>    <img src="https://icons.duckduckgo.com/i/www.amazon.com.ico" class="favicon">    Amazon    </a>        <a style="display: none;" href='' class='app_platform' data-title='{{title}}' data-artist='{{artist}}' data-song='{{song}}' id='itunes'>    <img src="https://icons.duckduckgo.com/i/www.apple.com.ico" class="favicon">    iTunes    </a>  </div>{{/checkMusic}}
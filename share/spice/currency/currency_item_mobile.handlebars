		</div>
		{{#if isPair}}
		<div class="xe_main_conv_mobile">
		<a class="xe_link" href="{{xeUrl}}">
			<h4>{{{amount}}} {{fromCurrencySymbol}} = {{convertedAmount}} {{toCurrencySymbol}}</h4>
			<div class="xe_line"></div>
			<span class="xe_rate">{{rate}}</span>
		</a>
		</div>
		{{else}}
		<div class="xe_main_conv_mobile">
		<a class="xe_link" href="{{xeUrl}}">
			<h4>{{{amount}}} {{fromCurrencySymbol}}</h4>	
			<div class="xe_xe_line"></div>
			<span class="xe_rate">{{rate}}</span>
		</a>
		</div>
		{{/if}}
	</div>
	<div class ="xe_timestamp">
		<p><a  href="http://www.xe.com"><strong>XE.com </strong></a> {{timeFreq}} mid-market rate: {{timeString}} UTC</p>
		<p><a  href="{{liveUrl}}"><strong>Visit live rates</strong></a></p>
	</div>
	{{/with}}

	<div class="tile tile--s ">
		{{#each topConvs}}
		<div class="xe_tc">
			<a class="xe_link" href="{{xeUrl}}">
			<div class="xe_tc_row1">
				<span class="xe_tc_col1"> {{toCurrencySymbol}}</span>
				<span class="xe_tc_col2">{{convertedAmount}}</span>
			</div>
			<div class="xe_tc_row2">
			<span class="xe_tc_col1"> {{currencyName}}</span>
			<span class="xe_tc_col2">{{rate}}</span>
			</div>
			</a>
		</div>
		{{/each}}
	</div>

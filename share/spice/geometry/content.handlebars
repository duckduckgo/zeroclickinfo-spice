<div id="zci--geometry-formulas" class="text--primary">
    {{#each formulas}}
        <div>
        <span>{{name}}:</span>
        {{symbol}} = {{{html}}}
        {{#if result}}
            = {{result}}
        {{/if}}
    </div>
    {{/each}}
    {{#if parameter}}
        <small>
            Assuming
            {{#each parameter}}
                {{symbol}} = {{value}}
            {{/each}}
        </small>
    {{/if}}
</div>
<svg id="zci--geometry-svg" viewBox="-5 -5 170 130" style="display: none">
    {{#each svg}}
        <path
            d="{{path}}"
            class="{{class}}"
        />
    {{/each}}
</svg>

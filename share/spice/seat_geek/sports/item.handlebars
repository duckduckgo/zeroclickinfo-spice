<div class="header_container">
     <h4 class="tx-clr--dk tile__title media__body has_sub">
         {{title}}
     </h4>
     {{#if logo}}
        <div class="container-wrapper">
            <div class="logo-container">
                <img src="{{logo}}" />
            </div>
        </div>
     {{/if}}
</div>
<div class="where">
    <div class="place-line one-line">
        {{place}}
    </div>
    <div class="city-line one-line">
        {{city}}
    </div>
</div>
<div class="tile__footer one-line">
   {{#if price}}
       <div class="price tx-clr--dk">{{price}}</div>
   {{else}}
       <div class="noprice tx-clr--lt">Price N/A</div>
   {{/if}}
   <div class="date">
        {{#if month}}
            {{month}}
        {{/if}}
        {{#if day}}
            {{day}}
        {{/if}}
    </div>
</div>
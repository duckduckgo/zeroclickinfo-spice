<h1 class="stocks__header  zci__header">
    <a href="{{url}}" title="{{urlTitle}}">
        {{Security.Name}}
    </a>
</h1>
<div class="stocks__sub  zci__header__sub">
    <span class="stocks__symbol">{{Security.Symbol}}</span>
    <span>as of {{date}} {{time}}</span>
    <span title="Eastern Time Zone"> ET</span>
</div>
<div class="stocks__quotes  stocks__quote-{{quoteChangeDir}}">
    <span class="stocks__quote text--primary">{{quote}}</span>
    <span class="stocks__change">{{change}}</span>
    <span class="stocks__change-sep  detail__sep"></span>
    <span class="stocks__change-pct">{{change_percent}}%</span>
</div>

<div class="c-base__links">
    {{{moreAt meta.sourceUrl meta.sourceName}}}
    <span class="zcm__sep"></span>
    <span class="zci__def__attribution  tx-clr--grey-dark  t-s">
        Data by <a href="https://xignite.com" class="tx-clr--grey-dark">{{meta.attributionText}}</a>
    </span>
</div>
<div>
    {{#each famlist from="0" to="1"}}
    <h1 class="zci__header"><a href='http://{{this}}'>{{this}}</a></h1>
    {{/each}}
    <div class='zci__subheader'>Owner: {{shortwho}}
    {{#if shortloc}}<span class='text--secondary iplookup_location'>({{shortloc}})</span>{{/if}}
    {{#if blacklists}}
            {{#concat blacklists sep=", " conj=" and "}}{{this}}{{/concat}}
    {{/if}}
    </div>
</div>
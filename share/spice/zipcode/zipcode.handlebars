{{#with relevantPlace}}
    <span>Zip Code {{name}} approximate area:</span>
    <a href="http://mapq.st/map?q={{this.centroid.latitude}},{{this.centroid.longitude}}">
        <img id="map" height="150" width="623" src="/imq2/?size=623,150&scalebar=false&bestfit={{bigbox boundingBox.northEast boundingBox.southWest}}&polygon=fill:0x30ff0000|color:0x20ff0000|width:1|{{coordString boundingBox.northEast boundingBox.southWest}}">
    </a>
{{/with}}

    <span>
        {{#checkZipcode}}
            {{#concat this sep="," conj=" and "}}<a href="/?q=postcode+{{name}}+{{country}}">{{country}}</a>{{/concat}}
        {{/checkZipcode}}
    </span>
<div class="card">
    <h3>{{name}}</h3>
    <h6>{{description}}</h6>
    <ul class="zci-cricket-list">
        {{#matches}}
            {{#each this}}
                <li>
                    {{date}}
                </li>
            {{/each}}
        {{/matches}}
        {{fullName 'a'}}
    </ul>
    <div>
        {{#matches}}
            <table class="zci-cricket-content">
                {{#each this}}
                    <tr>
                        <table class="zci-cricket-content">
                            <tr>
                                <td>{{mtype}}</td>
                                <td>
                                    {{#this.teams}}
                                        <span class="zci-cricket-team-name">{{this.Team}}</span>
                                    {{/this.teams}}
                                </td>
                                <td>{{this.startTime}}</td>
                            </tr>
                        </table>
                    </tr>
                {{/each}}
            </table>
        {{/matches}}
    </div>
    <div class="footer">
        <a id="prev" href="#" ripple="" ripple-color="#666666" class="chevron">
        </a>
        <a id="next" href="#" ripple="" ripple-color="#666666" class="chevron"></a>
    </div>
</div>

<div class="tile tile--f tile--get_events">
    <!-- Front of tile -->
    <div class="tile--f__main tile--get_events__main">
        <div class="tile__body has-foot--4">
            <div class="tile__title--wrap">
                <h5 class="tile__title with-sub tile__title--2">{{name}}</h5>
                <span class="tile__sub">
                    {{start}}
                    {{~#and start end}}
                    - {{end}}
                    {{/and~}}
                </span>
            </div>
            <div class="tile__num--get_events pull-right">
                <div class="number">
                    {{num}}
                </div>
            </div>
        </div>

        <div class="tile__foot tile__foot--4">
            <a class="tile__address">
                <div>{{venue.name}}</div>
                {{~#each address_lines~}}
                    <div>{{.}}</div>
                {{/each}}
            </a>
        </div>
    </div>

    <!-- Back of tile -->
    <div class="tile--f__alt tile--get_events__alt">
        <div class="tile__body  {{#and url meta.sourceName}}has-foot{{/and}}">

            <h5 class="tile__title tile__title--1">{{name}}</h5>

            <div class="tile__content tile__content--lg">
                {{~#if description~}}
                    {{description}}
                {{~else~}}
                    No description available.
                {{/if}}
            </div>

            {{#and url meta.sourceName}}
                <div class="tile__foot">
                    
                    {{{moreAt url meta.formattedSourceName dynamicMoreAtText=5}}}
                    
                </div>
            {{/and}}
        </div>
    </div>
</div>
<div class="tile tile--c has-detail tile--alternative_to opt--t-xxs">
  <div class="tile__media">
    <img src="{{imageProxy IconUrl}}" alt="{{Name}}" class="tile__media__img" />
  </div>
  <div class="tile__body">
    <h6 class="tile__title">{{ellipsis Name 26}}</h6>
    <div class="tile__rating  one-line">
      {{License}} <span>({{Votes}} likes)</span>
    </div>
  </div>
</div>
